{
    "manifest_version": 3,
    "name": "Grabby",
    "description": "Description for the extension",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Grabby"
  },
  "web_accessible_resources": [{
        "resources": ["grabby.html"],
        "matches": ["<all_urls>"]
    }]
}