{
    "manifest_version": 3,
    "name": "Grabby",
    "description": "Description for the extension",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "downloads"],
    "action": {
        "default_title": "Grabby",
        "default_popup": "popup.html"
    },
    "icons": { 
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png" 
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "web_accessible_resources": [{
        "resources": ["grabby.html"],
        "matches": ["<all_urls>"]
    }]
}